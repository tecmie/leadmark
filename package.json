{
  "name": "leadmark",
  "private": true,
  "scripts": {
    "build": "turbo run build",
    "build:worker": "turbo run build --filter=@repo/types --filter=@leadmark/worker ",
    "start:processor": "turbo run start --filter=@leadmark/worker",
    "dev": "turbo run dev",
    "start:worker": "cd apps/worker && pnpm start:prod",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "check-types": "turbo run check-types",
    "supabase:generate": "npx supabase gen types typescript --project-id isrjrkosodcxxejhsuuv --schema public > packages/types/src/database.types.ts"
  },
  "devDependencies": {
    "prettier": "^3.5.3",
    "turbo": "^2.5.3",
    "typescript": "5.8.2"
  },
  "packageManager": "pnpm@9.0.0",
  "engines": {
    "node": ">=18"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.49.8"
  }
}
